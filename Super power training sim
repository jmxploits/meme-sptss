	Default = false,
	Callback = function(Value)
		getgenv().JPV = Value
    JP()
	end    
})

MTab:AddToggle({
	Name = "Autofarm Psychic",
	Default = false,
	Callback = function(Value)
		getgenv().PSV = Value
    PS()
	end    
})

